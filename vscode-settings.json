{
    "workbench.colorCustomizations": {
        "editor.background": "#272829",
        "activityBarBadge.background": "#616161",
        "list.activeSelectionForeground": "#616161",
        "list.inactiveSelectionForeground": "#616161",
        "list.highlightForeground": "#616161",
        "scrollbarSlider.activeBackground": "#61616150",
        "editorSuggestWidget.highlightForeground": "#616161",
        "textLink.foreground": "#616161",
        "progressBar.background": "#616161",
        "pickerGroup.foreground": "#616161",
        "tab.activeBorder": "#616161",
        "notificationLink.foreground": "#616161",
        "editorWidget.resizeBorder": "#616161",
        "editorWidget.border": "#616161",
        "settings.modifiedItemIndicator": "#616161",
        "settings.headerForeground": "#616161",
        "panelTitle.activeBorder": "#616161",
        "breadcrumb.activeSelectionForeground": "#616161",
        "menu.selectionForeground": "#616161",
        "menubar.selectionForeground": "#b1afaf",
        "statusBar.background": "#292727",
        "statusBar.foreground": "#b3b0b0"
    },
    "window.zoomLevel": 0,
    "editor.minimap.enabled": false,
    "editor.fontFamily": "'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures":false,
    "editor.fontSize": 12,
    "editor.lineHeight": 26,
    "editor.letterSpacing": 0.60,
    "css.lint.duplicateProperties": "warning",
    "css.lint.argumentsInColorFunction": "warning",
    "css.lint.zeroUnits": "warning",
    "files.autoSave": "afterDelay",
    "explorer.confirmDelete": false,
    "git.autofetch": true,
    "window.titleBarStyle": "custom",
    "explorer.confirmDragAndDrop": false,
    "python.jediEnabled": false,
    "python.envFile": "${workspaceFolder}/.env",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
        {
            "scope": [
                "source.python",
                "variable.parameter.function-call.python",
                "entity.name.function.decorator.python",
                "entity.name.function.python",
                "variable.parameter.function.language.python",
                "support.function.magic.python",
                "entity.other.inherited-class.python"
            ],
            "settings": {
                "foreground": "#e9e9e9"
            }
        },
        {
            "scope": [
                "entity.name.type.class.python"
            ],
            "settings": {
                "foreground": "#e9e9e9",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "keyword.control.import.python",
                "storage.type.function.python",
                "keyword.control.flow.python",
                "constant.language.python",
                "keyword.operator.logical.python",
                "storage.type.function.lambda.python",
                "storage.type.class.python",
                "variable.parameter.function.language.special.self.python",
                "variable.language.special.self.python",
                "punctuation.separator.period.python"
            ],
            "settings": {
                "foreground": "#FF882F",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "meta.function-call.generic.python"
            ],
            "settings": {
                "foreground": "#FF882F"
            }
        },
        {
            "scope": [
                "string.quoted.single.python",
                "constant.character.format.placeholder.other.python"
            ],
            "settings": {
                "foreground": "#6FBF8C"
            }
        },
        {
            "scope": [
                "comment.line.number-sign.python",
                "string.quoted.docstring.multi.python"
            ],
            "settings": {
                "foreground": "#6d6d6d"
            }
        },
        {
            "scope": [
                "support.variable.magic.python",
                "support.function.builtin.python",
                "support.type.python"
            ],
            "settings": {
                "foreground": "#8888C6",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "constant.numeric.dec.python"
                
            ],
            "settings": {
                "foreground": "#7BAFD7",
                "fontStyle": "bold"
            }
        }
    ]
},
    "editor.suggestSelection": "first",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.rendererType": "dom",
    "liveServer.settings.donotVerifyTags": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "workbench.colorTheme": "Material Theme Darker High Contrast",
    "workbench.iconTheme": "material-icon-theme"
}

